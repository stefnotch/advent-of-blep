    "@theatre/studio": "^0.7.0",
    "@threlte/core": "^7.0.12",
    "@threlte/extras": "^8.0.10",
    "@threlte/theatre": "^2.1.5",
    "@types/seedrandom": "^3.0.8",
    "@types/three": "^0.159.0",
    "gh-pages": "^6.1.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-tweakpane-ui": "^1.0.2",
    "three": "^0.159.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
